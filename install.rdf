<?xml version="1.0" encoding="utf-8"?>
    <r:RDF xmlns="http://www.mozilla.org/2004/em-rdf#" xmlns:r="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
        <r:Description about="urn:mozilla:install-manifest">
            <creator>Hyperbola Project</creator>
            <contributor>Sibi Antony</contributor>
            <description>How strong is your HTTPS connection? SSLRank ranks an established SSL/TLS connection and provides a brief summary of the SSL/TLS parameters.</description>
            <homepageURL>https://pagure.io/sslrank</homepageURL>
            <id>sslrank@hyperbola.info</id>
            <name>SSLRank</name>
            <version>0.5.5</version>
            <iconURL>chrome://sslrank/skin/sslrank-icon.png</iconURL>
            <optionsURL>chrome://sslrank/content/preferences.xul</optionsURL>
            <optionsType>3</optionsType>

            <bootstrap>true</bootstrap>
            <type>2</type>
            <multiprocessCompatible>true</multiprocessCompatible>
            <!-- Basilisk / Iceweasel-UXP -->
            <targetApplication>
                <r:Description>
                    <id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</id>
                    <minVersion>52.0</minVersion>
                    <maxVersion>52.*</maxVersion>
                </r:Description>
            </targetApplication>
            <!-- Pale Moon -->
            <targetApplication>
                <r:Description>
                    <id>{8de7fcbb-c55c-4fbe-bfc5-fc555c87dbc4}</id>
                    <minVersion>28.0</minVersion>
                    <maxVersion>28.*</maxVersion>
                </r:Description>
            </targetApplication>

            <localized>
                <r:Description>
                    <locale>de-DE</locale>
                    <name>SSLRank</name>
                    <description>Wie stark ist Ihre HTTPS-Verbindung? SSLRank ordnet eine bestehende SSL / TLS-Verbindung ein und bietet eine kurze Zusammenfassung der SSL / TLS-Parameter.</description>
                </r:Description>
            </localized>

            <localized>
                <r:Description>
                    <locale>pl-PL</locale>
                    <name>SSLRank</name>
                    <description>Jak silne jest Twoje połączenie HTTPS? SSLRank ocenia nawiązane połączenie SSL/TLS i dostarcza krótkie podsumowanie parametrów SSL/TLS.</description>
                </r:Description>
            </localized>

        </r:Description>
    </r:RDF>
